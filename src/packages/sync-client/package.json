{
  "name": "@cocalc/sync-client",
  "version": "0.1.4",
  "description": "CoCalc Lightweight Nodejs Sync Client",
  "main": "./dist/lib/index.js",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist-esm/lib/index.js",
      "default": "./dist/lib/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist-esm/*.js",
      "default": "./dist/*.js"
    },
    "./lib/*": {
      "types": "./dist/lib/*.d.ts",
      "import": "./dist-esm/lib/*.js",
      "default": "./dist/lib/*.js"
    }
  },
  "files": [
    "dist/**",
    "dist-esm/**",
    "bin/**",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm exec tsc --build && pnpm exec tsc -p tsconfig-esm.json && echo '{\"type\":\"module\"}' > dist-esm/package.json",
    "clean": "rm -rf node_modules dist dist-esm",
    "depcheck": "pnpx depcheck"
  },
  "author": "SageMath, Inc.",
  "keywords": [
    "cocalc",
    "jupyter"
  ],
  "license": "SEE LICENSE.md",
  "dependencies": {
    "@cocalc/api-client": "workspace:*",
    "@cocalc/backend": "workspace:*",
    "@cocalc/primus-multiplex": "^1.1.0",
    "@cocalc/primus-responder": "^1.0.5",
    "@cocalc/sync": "workspace:*",
    "@cocalc/util": "workspace:*",
    "cookie": "^1.0.0",
    "debug": "^4.4.0",
    "primus": "^8.0.9",
    "ws": "^8.18.0"
  },
  "homepage": "https://github.com/sagemathinc/cocalc/tree/master/src/packages/sync-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/cocalc"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/debug": "^4.1.12",
    "@types/jest": "^30.0.0",
    "@types/node": "^18.16.14",
    "@types/primus": "^7.3.9"
  }
}
