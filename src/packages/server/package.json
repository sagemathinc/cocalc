{
  "name": "@cocalc/server",
  "version": "0.44.5",
  "description": "CoCalc server functionality: functions used by either the hub and the next.js server",
  "exports": {
    "./*": "./dist/*.js",
    "./auth/*": "./dist/auth/*.js",
    "./api/*": "./dist/api/*.js",
    "./hub/*": "./dist/hub/*.js",
    "./accounts/*": "./dist/accounts/*.js",
    "./bookmarks/*": "./dist/bookmarks/*.js",
    "./cloud": "./dist/cloud/index.js",
    "./database/*": "./dist/database/*.js",
    "./mentions/*": "./dist/mentions/*.js",
    "./conat": "./dist/conat/index.js",
    "./conat/api": "./dist/conat/api/index.js",
    "./conat/auth": "./dist/conat/auth/index.js",
    "./conat/file-server": "./dist/conat/file-server/index.js",
    "./conat/socketio": "./dist/conat/socketio/index.js",
    "./conat/project/*": "./dist/conat/project/*.js",
    "./control-agent": "./dist/control-agent/index.js",
    "./project-backup": "./dist/project-backup/index.js",
    "./purchases/*": "./dist/purchases/*.js",
    "./stripe/*": "./dist/stripe/*.js",
    "./projects/control": "./dist/projects/control/index.js",
    "./projects/control/*": "./dist/projects/control/*.js",
    "./projects/connection": "./dist/projects/connection/index.js",
    "./projects/*": "./dist/projects/*.js",
    "./settings": "./dist/settings/index.js",
    "./settings/*": "./dist/settings/*.js",
    "./test": "./dist/test/index.js",
    "./test/*": "./dist/test/*.js"
  },
  "keywords": [
    "utilities",
    "cocalc"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "rm -rf node_modules dist tsconfig.tsbuildinfo",
    "build": "../node_modules/.bin/tsc --build",
    "test": "pnpm test:pglite",
    "test:psql": "TZ=UTC jest --forceExit --maxWorkers=1",
    "test:pglite": "COCALC_TEST_USE_PGLITE=1 NODE_OPTIONS='--experimental-vm-modules' TZ=UTC jest --forceExit",
    "test:both": "pnpm test:psql && pnpm test:pglite",
    "depcheck": "pnpx depcheck",
    "prepublishOnly": "test"
  },
  "author": "SageMath, Inc.",
  "license": "SEE LICENSE.md",
