{
  "name": "@cocalc/ai",
  "version": "0.0.1",
  "description": "AI helpers (LangChain, LLM) shared between server and lite",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./llm": {
      "import": "./dist/llm/index.js",
      "require": "./dist/llm/index.js",
      "types": "./dist/llm/index.d.ts"
    },
    "./acp": {
      "import": "./dist/acp/index.js",
      "require": "./dist/acp/index.js",
      "types": "./dist/acp/index.d.ts"
    },
    "./control-agent": {
      "import": "./dist/control-agent/index.js",
      "require": "./dist/control-agent/index.js",
      "types": "./dist/control-agent/index.d.ts"
    },
    "./sync": {
      "import": "./dist/sync/index.js",
      "require": "./dist/sync/index.js",
      "types": "./dist/sync/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "pnpm exec tsc --build",
    "clean": "rm -rf dist node_modules tsconfig.tsbuildinfo",
    "test": "pnpm exec jest",
    "depcheck": "pnpm -w exec depcheck \"$PWD\" --ignores=@types/jest,@types/node"
  },
  "dependencies": {
    "@agentclientprotocol/sdk": "^0.5.1",
    "@cocalc/backend": "workspace:*",
    "@cocalc/conat": "workspace:*",
    "@cocalc/sync": "workspace:*",
    "@cocalc/util": "workspace:*",
    "@langchain/anthropic": "^1.1.0",
    "@langchain/core": "^1.0.6",
    "@langchain/google-genai": "^1.0.3",
    "@langchain/mistralai": "^1.0.0",
    "@langchain/ollama": "^1.0.1",
    "@langchain/openai": "^1.1.2",
    "awaiting": "^3.0.0",
    "lru-cache": "^7.18.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.10",
    "ts-jest": "^29.4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/cocalc"
  },
  "license": "SEE LICENSE.md"
}
