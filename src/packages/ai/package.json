{
  "name": "@cocalc/ai",
  "version": "0.0.1",
  "description": "AI helpers (LangChain, LLM) shared between server and lite",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./llm": {
      "import": "./dist/llm/index.js",
      "require": "./dist/llm/index.js",
      "types": "./dist/llm/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "pnpm exec tsc --build",
    "tsc": "pnpm exec tsc --watch --pretty --preserveWatchOutput",
    "clean": "rm -rf dist node_modules",
    "test": "pnpm exec jest"
  },
  "dependencies": {
    "@cocalc/backend": "workspace:*",
    "@cocalc/util": "workspace:*",
    "@langchain/anthropic": "^0.3.4",
    "@langchain/core": "^0.3.13",
    "@langchain/google-genai": "^0.1.5",
    "@langchain/mistralai": "^1.0.0",
    "@langchain/openai": "^0.3.3",
    "@langchain/ollama": "^0.1.6"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/lodash": "^4.17.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.16.14",
    "@types/pkg-dir": "^2.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/cocalc"
  },
  "license": "SEE LICENSE.md"
}
