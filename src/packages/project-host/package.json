{
  "name": "@cocalc/project-host",
  "version": "0.4.51",
  "description": "Multi-project host built on CoCalc Lite with embedded runner/file-server/proxy",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "bin": {
    "cocalc-project-host": "./bin/start.js"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm exec tsc --build",
    "test": "exit 0",
    "clean": "rm -rf node_modules dist tsconfig.tsbuildinfo build",
    "app": "pnpm run build && node dist/main.js",
    "daemon:start": "node scripts/daemon-start.js",
    "daemon:stop": "node scripts/daemon-stop.js",
    "build:bundle": "cd sea && ./build-bundle.sh",
    "build:tarball": "pnpm run build:bundle && cd build && tar Jcvf bundle.tar.xz bundle",
    "run:bundle": "node ./build/bundle/bundle/index.js",
    "sea": "pnpm run build:tarball && cd sea && ./build-sea.sh",
    "publish:sea": "cd sea && ./publish-sea.sh",
    "promote:sea": "cd sea && ./promote-sea.sh",
    "build:all": "pnpm run build && pnpm run build:tarball && pnpm run sea"
  },
  "files": [
    "dist/**",
    "bin/**",
    "scripts/**",
    "sea/**",
    "README.md",
    "package.json"
  ],
  "author": "SageMath, Inc.",
  "license": "SEE LICENSE.md",
  "dependencies": {
    "@cocalc/ai": "workspace:*",
    "@cocalc/backend": "workspace:*",
    "@cocalc/conat": "workspace:*",
    "@cocalc/file-server": "workspace:*",
    "@cocalc/lite": "workspace:*",
    "@cocalc/project-proxy": "workspace:*",
    "@cocalc/project-runner": "workspace:*",
    "@cocalc/util": "workspace:*",
    "@isaacs/ttlcache": "^1.4.1",
    "compression": "^1.8.1",
    "express": "^4.21.2",
    "micro-key-producer": "^0.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/node": "^22.10.10",
    "@vercel/ncc": "^0.38.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/cocalc"
  },
  "homepage": "https://github.com/sagemathinc/cocalc/tree/master/src/packages/project-host"
}
