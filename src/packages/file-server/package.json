{
  "name": "@cocalc/file-server",
  "version": "1.0.0",
  "description": "CoCalc File Server",
  "exports": {
    "./btrfs": "./dist/btrfs/index.js",
    "./btrfs/*": "./dist/btrfs/*.js",
    "./conat/*": "./dist/conat/*.js",
    "./fs/*": "./dist/fs/*.js"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm exec tsc --build",
    "tsc": "pnpm exec tsc  --watch  --pretty --preserveWatchOutput",
    "test": "pnpm exec jest  --maxWorkers=1 --forceExit",
    "depcheck": "pnpx depcheck",
    "clean": "rm -rf node_modules dist"
  },
  "files": ["dist/**", "README.md", "package.json"],
  "author": "SageMath, Inc.",
  "keywords": ["utilities", "btrfs", "cocalc"],
  "license": "SEE LICENSE.md",
  "dependencies": {
    "@cocalc/backend": "workspace:*",
    "@cocalc/conat": "workspace:*",
    "@cocalc/file-server": "workspace:*",
    "@cocalc/util": "workspace:*",
    "awaiting": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^18.16.14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/cocalc"
  },
  "homepage": "https://github.com/sagemathinc/cocalc/tree/master/src/packages/file-server"
}
