{
  "name": "@cocalc/file-server",
  "version": "1.0.0",
  "description": "CoCalc File Server",
  "exports": {
    "./btrfs": "./dist/btrfs/index.js",
    "./btrfs/*": "./dist/btrfs/*.js",
    "./conat/*": "./dist/conat/*.js",
    "./fs/*": "./dist/fs/*.js"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm exec tsc --build",
    "test": "pnpm exec jest  --maxWorkers=1 --forceExit",
    "depcheck": "pnpm -w exec depcheck \"$PWD\"",
    "clean": "rm -rf node_modules dist tsconfig.tsbuildinfo"
  },
  "files": [
    "dist/**",
    "README.md",
    "package.json"
  ],
  "author": "SageMath, Inc.",
  "keywords": [
    "utilities",
    "btrfs",
    "cocalc"
  ],
  "license": "SEE LICENSE.md",
  "dependencies": {
    "@cocalc/backend": "workspace:*",
    "@cocalc/conat": "workspace:*",
    "@cocalc/file-server": "workspace:*",
    "@cocalc/util": "workspace:*",
    "awaiting": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/cocalc"
  },
  "homepage": "https://github.com/sagemathinc/cocalc/tree/master/src/packages/file-server"
}
