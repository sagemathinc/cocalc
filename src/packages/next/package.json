{
  "name": "@cocalc/next",
  "version": "0.103.5",
  "description": "CoCalc's nextjs server",
  "main": "lib/init.js",
  "exports": {
    "./init": "./lib/init.js",
    "./lib/*": "./dist/lib/*.js",
    "./lib/share": "./dist/lib/share/*.js"
  },
  "files": [
    ".next/*",
    "styles/*",
    "public/*",
    "pages/*",
    "lib/*",
    "dist/*",
    "components/*",
    "README.md",
    "package.json",
    "tsconfig.json",
    "tsconfig-dist.json",
    "next-env.d.ts",
    "next.config.js"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "npx next telemetry disable && rm -rf .next && pnpm build-dev && NODE_OPTIONS='--max_old_space_size=8000' next build",
    "build-dev": "rm -rf dist && pnpm software && npx tsc --build tsconfig-dist.json",
    "tsc": "npx tsc --build tsconfig-dist.json -w --pretty --preserveWatchOutput ",
    "software": "bash ./software-inventory/setup.sh",
    "build-deps": "cd ../backend && pnpm build && cd ../util && pnpm build",
    "build-project": "pnpm build-deps && npx next telemetry disable && pnpm build-dev && NODE_OPTIONS='--max_old_space_size=8000' BASE_PATH=/$COCALC_PROJECT_ID/port/5000 next build",
    "clean": "rm -rf .next dist",
    "start": "unset PGHOST; pnpm exec next start",
    "start-project": "unset PGHOST PGUSER COCALC_ROOT; export PORT=5000 BASE_PATH=/$COCALC_PROJECT_ID/port/5000; echo https://cocalc.com$BASE_PATH; pnpm start",
    "test": "pnpm exec tsc --build tsconfig-dist.json && pnpm exec jest ./dist",
    "prepublishOnly": "pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sagemathinc/cocalc.git"
  },
  "author": "SageMath, Inc.",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/sagemathinc/cocalc/issues"
  },
  "homepage": "https://github.com/sagemathinc/cocalc/tree/master/src/packages/next",
  "private": false,
  "dependencies-comment": "IMPORTANT: we install typescript as a normal dependency (not devDependencies) since it may be needed at runtime due to server settings changing!!",
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@cocalc/assets": "workspace:*",
    "@cocalc/backend": "workspace:*",
    "@cocalc/cdn": "workspace:*",
    "@cocalc/database": "workspace:*",
    "@cocalc/frontend": "workspace:*",
    "@cocalc/server": "workspace:*",
    "@cocalc/util": "workspace:*",
    "@types/express": "^4.17.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vscode/vscode-languagedetection": "^1.0.22",
    "antd": "^5.8.3",
    "antd-img-crop": "^4.12.2",
    "async-await-utils": "^3.0.1",
    "awaiting": "^3.0.0",
    "base-64": "^1.0.0",
    "basic-auth": "^2.0.1",
    "cookies": "^0.8.0",
    "csv-stringify": "^6.3.0",
    "dayjs": "^1.11.7",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "lru-cache": "^7.14.1",
    "ms": "2.1.2",
    "next": "13.5.0",
    "next-remove-imports": "^1.0.11",
    "password-hash": "^1.2.2",
    "pg": "^8.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^2.1.0",
    "react-google-recaptcha-v3": "^1.9.7",
    "serve-index": "^1.9.1",
    "sha1": "^1.1.1",
    "sharp": "^0.30.5",
    "timeago-react": "^3.0.4",
    "tslib": "^2.3.1",
    "use-async-effect": "^2.2.7",
    "uuid": "^8.3.2",
    "xmlbuilder2": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.16.14",
    "react-test-renderer": "^18.2.0"
  }
}
