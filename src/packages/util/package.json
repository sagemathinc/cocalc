{
  "name": "@cocalc/util",
  "version": "1.77.2",
  "description": "CoCalc code shared between the frontend and the backend",
  "exports": {
    "./*": "./dist/*.js",
    "./db-schema": "./dist/db-schema/index.js",
    "./fill": "./dist/fill/index.js",
    "./types": "./dist/types/index.js",
    "./consts": "./dist/consts/index.js",
    "./consts/*": "./dist/consts/*.js",
    "./i18n": "./dist/i18n/index.js",
    "./sync/table": "./dist/sync/table/index.js",
    "./sync/editor/db": "./dist/sync/editor/db/index.js",
    "./purchases/quota/*": "./dist/purchases/quota/*.js",
    "./purchases/store/*": "./dist/purchases/store/*.js",
    "./redux/*": "./dist/redux/*.js",
    "./refcount/lease": "./dist/refcount/lease.js"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "rm -rf node_modules dist tsconfig.tsbuildinfo",
    "build": "pnpm exec tsc --build",
    "test": "pnpm exec jest",
    "depcheck": "pnpm -w exec depcheck \"$PWD\" --ignores events",
    "prepublishOnly": "pnpm test"
  },
  "files": [
    "dist/**",
    "bin/**",
    "README.md",
    "package.json"
  ],
  "author": "SageMath, Inc.",
  "keywords": [
    "utilities",
    "mathjax",
    "markdown",
    "cocalc"
  ],
  "license": "SEE LICENSE.md",
  "dependencies-COMMENT": "We must install react so that react-intl doesn't install the wrong version! See https://github.com/sagemathinc/cocalc/issues/8132",
  "dependencies": {
    "@ant-design/colors": "^6.0.0",
    "@cocalc/diff-match-patch": "^1.0.0",
    "@cocalc/util": "workspace:*",
    "@isaacs/ttlcache": "^1.4.1",
    "async": "^1.5.2",
    "awaiting": "^3.0.0",
    "dayjs": "^1.11.11",
    "decimal.js-light": "^2.5.1",
    "events": "3.3.0",
    "get-random-values": "^1.2.0",
    "immutable": "^4.3.0",
    "js-base64": "^3.7.7",
    "json-stable-stringify": "^1.0.1",
    "jsonic": "^1.0.1",
    "lodash": "^4.17.21",
    "lru-cache": "^7.18.3",
    "prop-types": "^15.7.2",
    "react": "^19.2.1",
    "react-intl": "^7.1.11",
    "redux": "^4.2.1",
    "reselect": "^4.1.8",
    "sha1": "^1.1.1",
    "underscore": "^1.12.1",
    "utility-types": "^3.10.0",
    "uuid": "^8.3.2",
    "voucher-code-generator": "^1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/cocalc"
  },
  "homepage": "https://github.com/sagemathinc/cocalc/tree/master/src/packages/util",
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.10.10",
    "@types/seedrandom": "^3.0.8",
    "@types/uuid": "^8.3.1",
    "expect": "^26.6.2",
    "seedrandom": "^3.0.5",
    "tsd": "^0.22.0"
  }
}
