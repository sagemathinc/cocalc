{
  "name": "@cocalc/cloud",
  "version": "0.1.2",
  "description": "Provider-agnostic cloud VM provisioning for project-hosts.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./catalog/*": "./dist/catalog/*.js",
    "./hyperstack/*": "./dist/hyperstack/*.js",
    "./lambda/*": "./dist/lambda/*.js",
    "./nebius/*": "./dist/nebius/*.js",
    "./gcp": "./dist/gcp.js",
    "./local": "./dist/local.js",
    "./types": "./dist/types.js",
    "./logger": "./dist/logger.js",
    "./*": "./dist/*.js"
  },
  "files": [
    "dist/**",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "pnpm exec tsc --build",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "sea": "pnpm run build && cd sea && ./build-sea.sh",
    "test": "pnpm exec jest",
    "build:connector": "cd self-host/go && ./build-connector.sh",
    "publish:connector": "cd self-host/go && ./publish-connector.sh"
  },
  "dependencies": {
    "@cocalc/backend": "workspace:*",
    "@cocalc/util": "workspace:*",
    "@google-cloud/compute": "^4.7.0",
    "@nebius/js-sdk": "^0.1.22",
    "awaiting": "^3.0.0",
    "axios": "^1.13.2"
  }
}
