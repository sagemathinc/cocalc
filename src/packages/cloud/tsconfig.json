{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "dist",
    "paths": {
      "@nebius/js-sdk/api/*": [
        "./node_modules/@nebius/js-sdk/dist/esm/api/*"
      ],
      "@nebius/js-sdk/runtime/*": [
        "./node_modules/@nebius/js-sdk/dist/esm/runtime/*"
      ]
    }
  },
  "exclude": ["node_modules", "dist"]
}
