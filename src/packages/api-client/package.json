{
  "name": "@cocalc/api-client",
  "version": "0.1.2",
  "description": "CoCalc api client - use cocalc from nodejs",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist-esm/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist-esm/*.js",
      "default": "./dist/*.js"
    },
    "./src/*": {
      "types": "./dist/src/*.d.ts",
      "import": "./dist-esm/src/*.js",
      "default": "./dist/src/*.js"
    }
  },
  "files": ["dist/**", "dist-esm/**", "bin/**", "README.md", "package.json"],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm exec tsc --build && pnpm exec tsc -p tsconfig-esm.json && echo '{\"type\":\"module\"}' > dist-esm/package.json",
    "clean": "rm -rf node_modules dist dist-esm",
    "depcheck": "pnpx depcheck --ignores @cocalc/api-client "
  },
  "author": "SageMath, Inc.",
  "keywords": ["cocalc", "jupyter"],
  "license": "SEE LICENSE.md",
  "dependencies": {
    "@cocalc/api-client": "workspace:*",
    "@cocalc/backend": "workspace:*",
    "@cocalc/util": "workspace:*"
  },
  "homepage": "https://github.com/sagemathinc/cocalc/tree/master/src/packages/compute",
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/cocalc"
  },
  "devDependencies": {
    "@types/node": "^18.16.14"
  }
}
