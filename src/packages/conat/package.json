{
  "name": "@cocalc/conat",
  "version": "1.0.0",
  "description": "Conat -- pub/sub framework.  Usable by both nodejs and browser.",
  "exports": {
    "./sync/*": "./dist/sync/*.js",
    "./llm/*": "./dist/llm/*.js",
    "./files/*": "./dist/files/*.js",
    "./ai/acp/*": "./dist/ai/acp/*.js",
    "./ai/acp/types": "./dist/ai/acp/types.js",
    "./socket": "./dist/socket/index.js",
    "./socket/*": "./dist/socket/*.js",
    "./sync-doc": "./dist/sync-doc/index.js",
    "./sync-doc/*": "./dist/sync-doc/*.js",
    "./hub/changefeeds": "./dist/hub/changefeeds/index.js",
    "./hub/api": "./dist/hub/api/index.js",
    "./hub/api/*": "./dist/hub/api/*.js",
    "./sync-files": "./dist/sync-files/index.js",
    "./service": "./dist/service/index.js",
    "./project/api": "./dist/project/api/index.js",
    "./project/mutagen": "./dist/project/mutagen/index.js",
    "./project/terminal": "./dist/project/terminal/index.js",
    "./project-host/api": "./dist/project-host/api.js",
    "./browser-api": "./dist/browser-api/index.js",
    "./*": "./dist/*.js"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm exec tsc --build",
    "clean": "rm -rf dist node_modules tsconfig.tsbuildinfo",
    "test": "pnpm exec jest",
    "depcheck": "pnpm -w exec depcheck \"$PWD\" --ignores events,bufferutil,utf-8-validate"
  },
  "files": [
    "dist/**",
    "README.md",
    "package.json"
  ],
  "author": "SageMath, Inc.",
  "keywords": [
    "utilities",
    "conat",
    "cocalc"
  ],
  "license": "SEE LICENSE.md",
  "dependencies": {
    "@cocalc/comm": "workspace:*",
    "@cocalc/conat": "workspace:*",
    "@cocalc/sync": "workspace:*",
    "@cocalc/util": "workspace:*",
    "@isaacs/ttlcache": "^1.4.1",
    "@msgpack/msgpack": "^3.1.1",
    "ascii-table3": "^1.0.1",
    "awaiting": "^3.0.0",
    "bufferutil": "^4.0.9",
    "dayjs": "^1.11.11",
    "events": "3.3.0",
    "immutable": "^4.3.0",
    "js-base64": "^3.7.7",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.21",
    "mime-types": "^3.0.1",
    "p-limit": "^4.0.0",
    "sha256": "^0.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "utf-8-validate": "^6.0.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.10.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/cocalc"
  },
  "homepage": "https://github.com/sagemathinc/cocalc/tree/master/src/packages/conat"
