{
  "name": "@cocalc/project-proxy",
  "version": "1.0.0",
  "description": "CoCalc Project Proxy (SSH/HTTP ingress)",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm exec tsc --build",
    "test": "pnpm exec jest  --maxWorkers=1 --forceExit",
    "depcheck": "pnpm -w exec depcheck \"$PWD\"",
    "clean": "rm -rf node_modules dist tsconfig.tsbuildinfo"
  },
  "files": [
    "dist/**",
    "README.md",
    "package.json"
  ],
  "author": "SageMath, Inc.",
  "keywords": [
    "cocalc",
    "proxy",
    "ssh",
    "websocket"
  ],
  "license": "SEE LICENSE.md",
  "dependencies": {
    "@cocalc/backend": "workspace:*",
    "@cocalc/util": "workspace:*",
    "@isaacs/ttlcache": "^1.4.1",
    "express": "^4.21.2",
    "http-proxy-3": "^1.23.2",
    "micro-key-producer": "^0.8.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/cocalc"
  },
  "homepage": "https://github.com/sagemathinc/cocalc/tree/master/src/packages/project-proxy"
}
