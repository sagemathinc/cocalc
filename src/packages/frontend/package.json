  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "rm -rf dist && pnpm i18n:compile && NODE_OPTIONS=--max-old-space-size=8192 tsc --build",
    "test": "jest",
    "depcheck": "pnpm -w exec depcheck \"$PWD\" --ignores cspell,@cspell/dict-typescript,events,@formatjs/cli,react-test-renderer,jest-environment-jsdom",
    "prepublishOnly": "pnpm test",
    "update-color-scheme": "node ./update-color-scheme.js",
    "clean": "rm -rf dist node_modules i18n/trans/*.compiled.json tsconfig.tsbuildinfo",
    "i18n:extract": "bash ./i18n/bin/extract.sh",
    "i18n:upload": "bash ./i18n/bin/upload.sh",
    "i18n:download": "bash ./i18n/bin/download.sh",
    "i18n:compile": "bash ./i18n/bin/compile.sh",
    "i18n:unused": "bash ./i18n/bin/unused.sh",
    "i18n:delete": "bash ./i18n/bin/delete.sh",
    "i18n:update": "pnpm i18n:extract && pnpm i18n:upload && pnpm i18n:download && pnpm i18n:compile",
    "spellcheck": "git ls-files \"*.ts\" \"*.tsx\" | xargs cspell --config cspell.json"
