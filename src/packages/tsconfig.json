{
  "compilerOptions": {
    "incremental": true,
    "jsx": "react",
    "target": "es5",
    "lib": ["es5", "es6", "es2017", "dom"],
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "allowJs": true,
    "sourceMap": true
  }
}
