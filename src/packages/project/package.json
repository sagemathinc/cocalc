{
  "name": "@cocalc/project",
  "version": "1.41.15",
  "description": "CoCalc: project daemon",
  "exports": {
    "./conat": "./dist/conat/index.js",
    "./conat/terminal": "./dist/conat/terminal/index.js",
    "./*": "./dist/*.js",
    "./project-info": "./dist/project-info/index.js"
  },
  "keywords": [
    "python",
    "sage",
    "sagemath",
    "latex",
    "terminal",
    "web",
    "ide",
    "math",
    "mathematics"
  ],
  "dependencies": {
    "@cocalc/backend": "workspace:*",
    "@cocalc/comm": "workspace:*",
    "@cocalc/conat": "workspace:*",
    "@cocalc/jupyter": "workspace:*",
    "@cocalc/primus-multiplex": "^1.1.0",
    "@cocalc/primus-responder": "^1.0.5",
    "@cocalc/project": "workspace:*",
    "@cocalc/sync": "workspace:*",
    "@cocalc/sync-client": "workspace:*",
    "@cocalc/util": "workspace:*",
    "@lydell/node-pty": "^1.1.0",
    "@types/jest": "^30.0.0",
    "awaiting": "^3.0.0",
    "commander": "^7.2.0",
    "daemonize-process": "^3.0.0",
    "diskusage": "^1.1.3",
    "express": "^4.21.2",
    "get-port": "^5.1.1",
    "http-proxy-3": "^1.23.2",
    "lodash": "^4.17.21",
    "prettier": "^3.6.2",
    "primus": "^8.0.9",
    "prom-client": "^15.1.3",
    "rimraf": "^5.0.5",
    "tmp": "0.2.4",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.10.10",
    "@types/primus": "^7.3.9",
    "@vercel/ncc": "^0.38.4"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "NODE_OPTIONS='--trace-warnings --unhandled-rejections=strict --enable-source-maps' pnpm cocalc-project",
    "build": "../node_modules/.bin/tsc --build",
    "test": "COCALC_PROJECT_ID=812abe34-a382-4bd1-9071-29b6f4334f03 COCALC_USERNAME=user NODE_OPTIONS='--experimental-vm-modules' pnpm exec jest",
    "depcheck": "pnpm -w exec depcheck \"$PWD\" --ignores=@vercel/ncc",
    "prepublishOnly": "pnpm test",
    "clean": "rm -rf dist build tsconfig.tsbuildinfo",
    "build:bundle": "cd sea && ./build-bundle.sh && cd ../build && OS=$(uname -s | tr '[:upper:]' '[:lower:]') && ARCH=$(uname -m) && case \"$ARCH\" in x86_64|amd64) ARCH=amd64;; aarch64|arm64) ARCH=arm64;; esac && tar Jcvf bundle-${OS}-${ARCH}.tar.xz bundle",
    "build:tools": "cd sea && ./build-tools.sh",
    "run:bundle": "node ./build/bundle/bundle/index.js",
    "sea": "pnpm build:tarball && cd sea && ./build-sea.sh",
    "publish:bundle": "cd sea && ./publish-bundle.sh",
    "publish:tools": "cd sea && ./publish-tools.sh",
    "publish:sea": "cd sea && ./publish-sea.sh",
    "promote:sea": "cd sea && ./promote-sea.sh",
    "build:all": "pnpm build && pnpm build:tarball && pnpm build:sea"
  },
  "author": "SageMath, Inc.",
  "contributors": [
    "William Stein <wstein@sagemath.com>"
  ],
  "license": "SEE LICENSE.md",
  "bugs": {
    "url": "https://github.com/sagemathinc/cocalc/issues"
  },
  "homepage": "https://github.com/sagemathinc/cocalc/tree/master/src/packages/project",
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/cocalc"
  },
  "bin": {
    "cocalc-project": "./bin/cocalc-project.js"
  }
}
