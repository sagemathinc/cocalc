{
  "name": "@cocalc/static",
  "workspaces": [
    "../assets",
    "../backend",
    "../cdn",
    "../frontend",
    "../util"
  ],
  "version": "1.123.1",
  "description": "CoCalc's static frontend Webpack-based build system and framework",
  "main": "./src/index.js",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx"
    ]
  },
  "scripts": {
    "webpack": "NODE_OPTIONS=--max_old_space_size=8000  NODE_ENV=development npx webpack --progress --color --watch",
    "tsc": "cd src && npx tsc --watch --pretty --preserveWatchOutput --noEmit",
    "webpack-prod": "NODE_OPTIONS=--max_old_space_size=8000 NODE_ENV=production npx webpack --progress --color --watch",
    "webpack-measure": "COCALC_OUTPUT=dist-measure MEASURE=true NODE_ENV=development npm run webpack ",
    "webpack-measure-prod": "COCALC_OUTPUT=dist-prod-measure MEASURE=true npm run webpack-prod",
    "clean": "rm -rf dist",
    "build": "npm run clean && ./production-build.py"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/cocalc"
  },
  "keywords": [
    "python",
    "sage",
    "sagemath",
    "latex",
    "terminal",
    "web",
    "ide",
    "math",
    "mathematics"
  ],
  "author": "SageMath, Inc.",
  "license": "SEE LICENSE.md",
  "bugs": {
    "url": "https://github.com/sagemathinc/cocalc/issues"
  },
  "homepage": "https://github.com/sagemathinc/cocalc/tree/master/src/packages/static",
  "dep-notes": "WARNING: is-plain-object dep is needed for slate in frontend, or an ancient version gets installed here.",
  "devDependencies": {
    "@cocalc/assets": "^1.9.1",
    "@cocalc/backend": "^1.19.1",
    "@cocalc/cdn": "^1.14.1",
    "@cocalc/frontend": "^1.89.1",
    "@cocalc/util": "^1.67.1",
    "@mapbox/node-pre-gyp": "^1.0.9",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^28.1.6",
    "@types/jquery": "^3.5.5",
    "@types/node": "^14.18.10",
    "@types/react": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "assert": "^2.0.0",
    "async": "^2.6.3",
