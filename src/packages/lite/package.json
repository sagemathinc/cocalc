{
  "name": "@cocalc/lite",
  "version": "0.2.4",
  "description": "CoCalc Lite Standalone Server that Optional Syncs with an existing CoCalc instance",
  "exports": {
    "./*": "./dist/*.js"
  },
  "keywords": ["lite", "cocalc"],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "rm -rf build dist node_modules sea/sea-prep.blob",
    "build": "pnpm exec tsc --build",
    "tsc": "pnpm exec tsc --watch  --pretty --preserveWatchOutput",
    "test": "exit 0",
    "depcheck": "pnpx depcheck --ignores @types/debug,@types/jest,@types/node,@vercel/ncc",
    "build:static": "cd ../static && pnpm clean && pnpm install && pnpm build",
    "build:bundle": "pnpm build:static && cd sea && ./build-bundle.sh",
    "build:tarball": "pnpm build:bundle && cd build && tar Jcvf bundle.tar.xz bundle",
    "run:bundle": "node ./build/bundle/bundle/index.js",
    "sea": "cd sea && ./build-sea.sh",
    "build:all": "pnpm build && pnpm build:tarball && pnpm sea",
    "app": "pnpm exec cocalc-lite",
    "app-electron": "pnpm electron-rebuild && electron ."
  },
  "bin": {
    "cocalc-lite": "./bin/start.js"
  },
  "author": "SageMath, Inc.",
  "license": "SEE LICENSE.md",
  "dependencies": {
    "@cocalc/assets": "workspace:*",
    "@cocalc/backend": "workspace:*",
    "@cocalc/ai": "workspace:*",
    "@cocalc/conat": "workspace:*",
    "@cocalc/lite": "workspace:*",
    "@cocalc/project": "workspace:*",
    "@cocalc/static": "workspace:*",
    "@cocalc/util": "workspace:*",
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.23",
    "@types/lodash": "^4.17.20",
    "compression": "^1.8.1",
    "express": "^4.21.2",
    "formidable": "^3.5.4",
    "http-proxy-3": "^1.22.0",
    "lodash": "^4.17.21",
    "selfsigned": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/cocalc"
  },
  "homepage": "https://github.com/sagemathinc/cocalc/tree/master/src/packages/test",
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/node": "^18.16.14",
    "@vercel/ncc": "^0.38.4",
    "electron": "^37.3.1"
  }
}
