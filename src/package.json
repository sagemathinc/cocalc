{
  "name": "sagemathcloud",
  "version": "1.0.0",
  "description": "A collaborative web-based interface to Sage, IPython, LaTeX and the Terminal.",
  "main": "index.js",
  "directories": {
    "doc": "doc"
  },
  "dependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "cjsx-loader": "^3.0.0",
    "clean-webpack-plugin": "^0.1.9",
    "coffee-cache": "^1.0.2",
    "coffee-loader": "^0.7.2",
    "coffee-react": "^5.0.0",
    "coffee-react-transform": "^4.0.0",
    "coffee-script": "^1.10.0",
    "coffeelint": "^1.15.7",
    "coffeelint-cjsx": "2.0.2",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.1.4",
    "html-loader": "^0.4.2",
    "html-minify-loader": "^1.1.0",
    "html-webpack-plugin": "^2.10.0",
    "imports-loader": "^0.6.5",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-cjsx": "^1.0.0",
    "node-glob": "^1.2.0",
    "node-sass": "^3.7.0",
    "sass-loader": "^3.2.0",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.6.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-path-rewriter": "^1.1.3",
    "webpack-sha-hash": "^1.0.0",
    "webpack-stats-plugin": "^0.1.1"
  },
  "scripts": {
    "lint": "node_modules/.bin/coffeelint -f smc-util/coffeelint.json -c *.coffee && cd smc-hub && npm run lint && cd ../smc-webapp && npm run lint && cd ../smc-util && npm run lint && cd ../smc-util-node && npm run lint",
    "test": "export SMC_TEST=true&& cd smc-util && npm test && cd ../smc-util-node && npm test && cd ../smc-hub && npm test && cd ../smc-webapp && npm test",
    "coverage": "cd smc-util && npm run coverage && cd ../smc-util-node && npm run coverage && cd ../smc-hub && npm run coverage && cd ../smc-webapp && npm run coverage",
    "webpack": "cd $SALVUS_ROOT; webpack -d --progress --colors",
    "webpack-watch": "cd $SALVUS_ROOT; webpack --debug --output-pathinfo --progress --colors --watch",
    "webpack-watch-quick": "cd $SALVUS_ROOT; SMC_WEBPACK_QUICK=true webpack --debug --output-pathinfo --progress --colors --watch",
    "webpack-watch-map": "cd $SALVUS_ROOT; SOURCE_MAP=true webpack --debug --output-pathinfo --progress --colors --watch",
    "webpack-watch-quick-map": "cd $SALVUS_ROOT; SMC_WEBPACK_QUICK=true SOURCE_MAP=true webpack --debug --output-pathinfo --progress --colors --watch",
    "webpack-production": "cd $SALVUS_ROOT; NODE_ENV=production webpack --progress --colors",
    "webpack-clean": "rm -rvf $SALVUS_ROOT/static/",
    "install-all": "scripts/smc-install-all",
    "make": "npm run install-all",
    "clean": "rm -rf node_modules */node_modules static/webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sagemathinc/smc"
  },
  "keywords": [
    "python",
    "sage",
    "sagemath",
    "latex",
    "terminal",
    "web",
    "ide",
    "math",
    "mathematics"
  ],
  "author": "SageMath, Inc.",
  "contributors": [
    "William Stein <wstein@sagemath.com>",
    "Harald Schilly <hsy@sagemath.com>",
    "Jon Lee <jlee27@uw.edu>",
    "Nich Ruhland <nicholasruhland@gmail.com>"
  ],
  "license": "GPL-3.0+",
  "bugs": {
    "url": "https://github.com/sagemathinc/smc/issues"
  },
  "homepage": "https://github.com/sagemathinc/smc"
}
